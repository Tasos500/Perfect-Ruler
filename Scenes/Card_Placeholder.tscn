code = "shader_type canvas_item;

uniform bool spellbound = false;

void fragment(){
	if (spellbound==true){
		vec4 color = texture(TEXTURE, UV);
		COLOR = vec4(1.0 - color.rgb, color.a);
		}
	else{
		vec4 color = texture(TEXTURE, UV);
		COLOR = vec4(color.rgb, color.a);
		}
}"

